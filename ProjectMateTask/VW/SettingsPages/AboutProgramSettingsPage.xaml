<UserControl
    d:DataContext="{d:DesignInstance {x:Type additionalPages:AboutProgramVmd},
                                     IsDesignTimeCreatable=True}"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d"
    x:Class="ProjectMateTask.VW.SettingsPages.AboutProgramSettingsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:additionalPages="clr-namespace:ProjectMateTask.VMD.Pages.SettingsVmds"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ProjectMateTask.VW.SettingsPages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Тестовое задние ProjectMate" />
            <TextBlock Text="Создатель: Константин" />
            <TextBlock Text="{Binding AppVersion, StringFormat=Версия приложения: {0}}" />
        </StackPanel>
    </Grid>
</UserControl>
