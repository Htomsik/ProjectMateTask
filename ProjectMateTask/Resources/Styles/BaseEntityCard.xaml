<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:base="clr-namespace:ProjectMateTask.DAL.Entities.Base;assembly=ProjectMateTask.DAL"
    xmlns:collections="clr-namespace:System.Collections;assembly=System.Runtime"
    xmlns:componentModel="clr-namespace:System.ComponentModel;assembly=WindowsBase"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Для всех Entity если не переопределен  -->
    <DataTemplate DataType="{x:Type base:NamedEntity}">
        <VirtualizingStackPanel Orientation="Vertical">
            <TextBlock Style="{StaticResource MaterialDesignSubtitle1TextBlock}" Text="{Binding Name}" />
        </VirtualizingStackPanel>
    </DataTemplate>

</ResourceDictionary>